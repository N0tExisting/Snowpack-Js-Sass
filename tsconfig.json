{
	"compilerOptions": {
		"declaration": true,
		"baseUrl": "./",
		"inlineSourceMap": true,
		"inlineSources": true,
		"declarationMap": true,
		"target": "es2019",
		"lib": [
			"ES2019"
		],
		"module": "CommonJS",
		"noImplicitAny": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "./out",
		"moduleResolution": "node"
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"./original/**/*"
	]
}